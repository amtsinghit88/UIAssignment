<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Mobile purchase test scenario" verbose="3">

	<listeners>

		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	
<test name="Mobile purchase-chrome">

	<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.shadow.ui.MobilePurchase">
				<methods>
					<include name="mobilePurchase"></include>
				</methods>

			</class>
		</classes>
	</test>
